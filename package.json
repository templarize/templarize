{
  "name": "templarize",
  "version": "0.1.0",
  "description": "A package to quickly build from template files",
  "bin": {
    "templarize": "dist/index.js"
  },
  "scripts": {
    "dev": "ts-node src/index.ts",
    "build": "tsc",
    "prepare": "npm run build",
    "format": "prettier --write 'src/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/minagishl/templarize.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "templarize"
  ],
  "author": "minagishl",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/minagishl/templarize/issues"
  },
  "homepage": "https://github.com/minagishl/templarize#readme",
  "devDependencies": {
    "@types/node": "^20.11.25",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2"
  }
}
